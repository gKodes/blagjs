{
  "name": "@blag/core",
  "version": "0.0.0",
  "description": "> TODO: description",
  "author": "Kamalakar Gadireddy <Kamalakar.Gadireddy@marriott.com>",
  "homepage": "https://github.com/gKodes/blagjs/blob/master/packages/core/README.md",
  "license": "MIT",
  "main": "es/index.js",
  "typings": "index.d.ts",
  "resolveUsing": "import",
  "exports": {
    "./": {
      "require": "./",
      "import": "./es/"
    }
  },
  "directories": {
    "lib": "es",
    "test": "__tests__"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gKodes/blagjs.git"
  },
  "scripts": {
    "test": "jest --color",
    "build": "babelc es",
    "prepack": "cp package.json index.d.ts dist"
  },
  "bugs": {
    "url": "https://github.com/gKodes/blagjs/issues"
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleDirectories": [
      "node_modules",
      "node_modules/@blag/babel/node_modules"
    ],
    "coverageReporters": [
      "text",
      "json",
      "lcov",
      "html"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "transform": {
      "^.+\\.[t|j]sx?$": "@blag/babel/jest"
    },
    "testEnvironment": "node",
    "verbose": true,
    "resolver": "@blag/babel"
  },
  "devDependencies": {
    "@blag/babel": "file:../babel",
    "jest": "^26.0.1",
    "jest-cli": "^26.0.1"
  },
  "dependencies": {
    "qs": "^6.9.4",
    "url-parse": "^1.4.7"
  }
}
